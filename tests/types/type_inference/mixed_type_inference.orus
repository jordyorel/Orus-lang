// Test type inference with same types (exact type matching)

// f64 arithmetic
let a: f64 = 10.0
let b: f64 = 2.5
let result1 = a + b  // Should infer f64
print(result1)

// u32 arithmetic
let x = 100u32  
let y = 50u32
let result2 = x + y  // Should infer u32
print(result2)

// i32 arithmetic (default type)
let p = 25
let q = 5
let result3 = p * q  // Should infer i32
print(result3)

// i64 arithmetic
let m = 42i64
let n = 8i64
let result4 = m + n  // Should infer i64
print(result4)