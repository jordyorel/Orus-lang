// Comprehensive u32 arithmetic type inference tests
let a = 10u32
let b = 5u32

// All basic operations should infer u32 type
let addition = a + b
let subtraction = a - b  
let multiplication = a * b
let division = a / b
let modulo = a % b

print(addition)      // 15
print(subtraction)   // 5
print(multiplication) // 50
print(division)      // 2
print(modulo)        // 0

// Chained operations should also infer u32
let complex = (a + b) * (a - b)
print(complex)

// With larger values
let x = 4000000000u32
let y = 2u32
let result = x / y
print(result)