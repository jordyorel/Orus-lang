// Test static vs dynamic loop analysis
print("=== STATIC VS DYNAMIC ANALYSIS TESTS ===")

print("Test 1: Static for-loop (should be analyzed at compile time)")
mut sum1 = 0
for i in 0..150000:  // Static range > 100K, should have guards
    sum1 = sum1 + i
print("Static for-loop result:", sum1)

print("Test 2: Dynamic for-loop (runtime analysis)")
limit = 150000  // Dynamic variable
mut sum2 = 0
for i in 0..limit:  // Dynamic range, should have guards
    sum2 = sum2 + i
print("Dynamic for-loop result:", sum2)

print("Test 3: Static while-loop (compile-time constant)")
mut count3 = 0
while count3 < 150000:  // Static comparison, but while-loops are usually dynamic
    count3 = count3 + 1
print("Static while-loop result:", count3)

print("Test 4: Dynamic while-loop (runtime variable)")
target = 150000
mut count4 = 0
while count4 < target:  // Dynamic comparison
    count4 = count4 + 1
print("Dynamic while-loop result:", count4)

print("All static/dynamic tests completed!")