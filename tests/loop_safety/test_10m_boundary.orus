// Test 10M boundary edge cases
print("=== 10M BOUNDARY TESTS ===")

print("Test 1: Exactly 9,999,999 iterations (just under 10M limit)")
mut count1 = 0
while count1 < 9999999:
    count1 = count1 + 1
    if count1 % 1000000 == 0:
        print("Progress:", count1)
print("9,999,999 result:", count1)

print("Test 2: Exactly 10,000,000 iterations (at 10M limit)")
mut count2 = 0
while count2 < 10000000:
    count2 = count2 + 1
    if count2 % 1000000 == 0:
        print("Progress:", count2)
print("10,000,000 result:", count2)

print("This should not print if 10M limit works")