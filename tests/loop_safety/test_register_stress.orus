// Test register allocation under stress
print("=== REGISTER ALLOCATION STRESS TESTS ===")

print("Test 1: Many variables with guards")
a1 = 1
a2 = 2
a3 = 3
a4 = 4
a5 = 5
a6 = 6
a7 = 7
a8 = 8
a9 = 9
a10 = 10

mut count1 = 0
while count1 < 200000:  // Should trigger guards
    temp = a1 + a2 + a3 + a4 + a5 + a6 + a7 + a8 + a9 + a10
    count1 = count1 + temp
print("Many variables result:", count1)

print("Test 2: Deep nested loops with guards")
mut sum = 0
for i in 0..100:
    for j in 0..100:
        for k in 0..20:  // Total: 100*100*20 = 200K iterations
            sum = sum + i + j + k
print("Deep nested result:", sum)

print("Test 3: Multiple guard registers")
mut count3a = 0
while count3a < 150000:  // First guarded loop
    count3a = count3a + 1

mut count3b = 0  
while count3b < 150000:  // Second guarded loop
    count3b = count3b + 1

print("Multiple guards result:", count3a, count3b)

print("All stress tests completed!")